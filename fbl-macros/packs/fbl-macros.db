{"name":"Critical Injuries","permission":{"default":0,"QIPPs5GkUEG0G4jy":3},"type":"script","flags":{"furnace":{"runAsGM":false}},"scope":"global","command":"let damage = \"\";\r\nfunction printInjury (result) {\r\n let chatData = {\r\n  content: `<div class=\"forbidden-lands chat-item\" style=\"text-align:center\"><div class=\"border\"><h3>Critical Injury</h3><h4>You suffer a ${damage} wound:</h4><p style=\"padding:10px\">@${result.collection}[${result.resultId}]</p><p>Result: ${result.range[0]} - ${result.range[1]}</p></div></div>`,\r\n  card: game.items.get(result.resultId).sendToChat(),\r\n  };\r\n ChatMessage.create(chatData, {});\r\n}\r\nlet dType = new Dialog({\r\n\ttitle: \"Damage Type\",\r\n\tcontent: \"<h3>What was the last type of damage done?</h3>\",\r\n\tbuttons: {\r\n\t\tblunt: {\r\n\t\t\tlabel: \"Blunt\",\r\n\t\t\tcallback : () => {\r\n\t\t\t\tdamage += \"blunt\";\r\n\t\t\t\tprintInjury(game.tables.entities.find(t => t.name === \"Critical Injuries - Blunt Wounds\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\r\n\t\tslashing: {\r\n\t\t\tlabel: \"Slashing\",\r\n\t\t\tcallback : () => {\r\n\t\t\t\tdamage += \"slash\";\r\n\t\t\t\tprintInjury(game.tables.entities.find(t => t.name === \"Critical Injuries - Slash Wounds\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\r\n\t\tstabbing: {\r\n\t\t\tlabel: \"Stab\",\r\n\t\t\tcallback: () => {\r\n\t\t\t\tdamage += \"stab\";\r\n\t\t\t\tprintInjury(game.tables.entities.find(t => t.name === \"Critical Injuries - Stab Wounds\").roll().results[0]);\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}).render(true);","author":"QIPPs5GkUEG0G4jy","img":"modules/game-icons-net/whitetransparent/sword-wound.svg","actorIds":[],"_id":"1KmUX4it1EA2m2Bf"}
{"name":"Init: Deal to Player","permission":{"default":0,"B5LOlNLF1n0tx9SW":3},"type":"script","flags":{"furnace":{"runAsGM":true}},"scope":"global","command":"let num = 0;\r\nconst buttons = {}\r\nconst initiativeDeck = game.decks.get(game.data.folders.find(n => n.name == \"Initiative\")._id)\r\n\r\nfor (let i = 0; i < game.users.size; i++) {\r\n    buttons[`btn_${i}`] = { \r\n        label: `${game.users.entries[i].name.toString()}`, \r\n        callback: () => { \r\n         initiativeDeck.dealToPlayer(game.users.entries[i]._id, num, 0)\r\n        }\r\n    }\r\n}\r\n\r\nlet dPlayer = new Dialog({\r\n    title: \"Player\",\r\n    content: \"<h3>Which player to deal to?</h3>\",\r\n    buttons: buttons,\r\n})\r\n\r\nnew Dialog({\r\n title: \"Number of cards\",\r\n content: \"<h3>How many initiative cards?</h3>\",\r\n buttons: {\r\n  one: { \r\n   label: \"1\",\r\n   callback: () => {\r\n    num = 1;\r\n    dPlayer.render(true);\r\n   }\r\n  },\r\n  two: {\r\n   label: \"2\",\r\n   callback: () => {\r\n    num = 2;\r\n    dPlayer.render(true);\r\n   }\r\n  },\r\n  three: {\r\n   label: \"3\",\r\n   callback: () => {\r\n    num = 3;\r\n    dPlayer.render(true);\r\n   }\r\n  },\r\n}}).render(true)\r\n","author":"B5LOlNLF1n0tx9SW","img":"modules/game-icons-net/whitetransparent/card-draw.svg","actorIds":[],"_id":"4NaUofqdOGSr4MOB"}
{"name":"Journeying Mishaps","permission":{"default":0,"QIPPs5GkUEG0G4jy":3},"type":"script","flags":{"furnace":{"runAsGM":false}},"scope":"global","command":"let message;\r\nfunction printMishap(result) {\r\n let chatData = {\r\n\t\tcontent: `<div class=\"forbidden-lands chat-item\" style=\"text-align:center\"><div class=\"border\"><h3>Journeying Mishap </h3><h4>While ${message}, you suffer:</h4><p style=\"padding:10px\">${result.text}</p><p>Result: ${result.range[0]} - ${result.range[1]}</p></div></div>`,\r\n\t};\r\n\tChatMessage.create(chatData, {});\r\n}\r\nlet dType = new Dialog({\r\n\ttitle: \"Activity\",\r\n\tcontent: \"<h3>What journeying activity was rolled?</h3>\",\r\n\tbuttons: {\r\n\t\tfish: {\r\n                        label: \"Fish\",\r\n\t\t\tcallback : () => {\r\n\t\t\tmessage = \"fishing\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Fish Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\r\n\t\tforage: {\r\n                        label: \"Forage\",\r\n\t\t\tcallback: () => {\r\n\t\t\tmessage = \"foraging\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Forage Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\t\t\r\n                hunting: {\r\n                        label: \"Hunt\",\r\n\t\t\tcallback: () => {\r\n\t\t\tmessage = \"hunting\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Hunting Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\t\t\r\n                leadway: {\r\n\t\t\tlabel: \"Lead\",\r\n\t\t\tcallback: () => {\r\n                                message = \"leading the way\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Lead the Way Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\t\t\r\n                makecamp: {\r\n\t\t\tlabel: \"Camp\",\r\n\t\t\tcallback: () => {\r\n                        message = \"making camp\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Make Camp Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t},\t\t\r\n                seatravel: {\r\n\t\t\tlabel: \"Sea\",\r\n\t\t\tcallback: () => {\r\n                        message = \"travelling at sea\";\r\n\t\t\t\tprintMishap(game.tables.entities.find(t => t.name === \"Sea Travel Mishaps\").roll().results[0]);\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}).render(true);","author":"QIPPs5GkUEG0G4jy","img":"icons/svg/bones.svg","actorIds":[],"_id":"GpJxQ2k89Zr6P6G5"}
